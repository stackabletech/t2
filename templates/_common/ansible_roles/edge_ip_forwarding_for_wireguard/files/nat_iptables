*filter
:INPUT ACCEPT
:FORWARD ACCEPT
:OUTPUT ACCEPT
-A INPUT -i ens6 -p udp -m udp --dport 52888 -j ACCEPT
-A INPUT -i wg -j ACCEPT
-A FORWARD -i wg -o ens6 -j ACCEPT
-A FORWARD -i ens6 -o wg -j ACCEPT
COMMIT
*nat
:PREROUTING ACCEPT
:INPUT ACCEPT
:OUTPUT ACCEPT
:POSTROUTING ACCEPT 
-A POSTROUTING -s 10.11.12.0/24 -o ens6 -j MASQUERADE
-A POSTROUTING -o ens6 -j MASQUERADE
COMMIT
