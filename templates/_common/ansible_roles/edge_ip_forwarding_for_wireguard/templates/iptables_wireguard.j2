*filter
:INPUT ACCEPT
:FORWARD ACCEPT
:OUTPUT ACCEPT
-A INPUT -i {{ public_network_interface_name }} -p udp -m udp --dport 52888 -j ACCEPT
-A INPUT -i wg -j ACCEPT
-A FORWARD -i wg -o {{ private_network_interface_name }} -j ACCEPT
-A FORWARD -i {{ private_network_interface_name }} -o wg -j ACCEPT
-A FORWARD -o {{ public_network_interface_name }} -j ACCEPT
COMMIT
*nat
:PREROUTING ACCEPT
:INPUT ACCEPT
:OUTPUT ACCEPT
:POSTROUTING ACCEPT 
-A POSTROUTING -s 10.11.12.0/24 -o {{ private_network_interface_name }} -j MASQUERADE
-A POSTROUTING -o {{ public_network_interface_name }} -j MASQUERADE
COMMIT
